{
    "name": "tourdefran/youtubetwigembed",
    "description": "Allows you to embed Youtube and Vimeo videos on Markdown (Parsedown Extra).",
    "type": "bolt-extension",
    "keywords": [
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Franco Arias",
            "email": "francoarias@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\tourdefran\\youtubetwigembed\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\tourdefran\\youtubetwigembed\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\tourdefran\\youtubetwigembed\\youtubetwigembedExtension"
    }
}
